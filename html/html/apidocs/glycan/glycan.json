{
  "swagger": "2.0",
  "info": {
    "description": "The GlyGen REST API provides access to glycobiology data from GlyGen. The services provide programmatic data access to integrated glycan, protein and proteoform data.",
    "version": "0.0.1",
    "title": "GlyGen Glycan APIs",
    "contact": {
      "email": "help@glygen.org"
    },
    "license": {
      "name": "MIT",
      "url": "https://glygen.org/LICENSE"
    }
  },
  "host": "glygen-vm-tst.biochemistry.gwu.edu",
  "basePath": "/api",
  "produces": ["application/json"],
  "schemes": ["http"],
  "paths": {
        "/glycan/search/?query={query_json}": {
              "get": {
                "tags": [
                    "Glyan Search"
                ],
                "description": "Search for glycans",
                "parameters": [
                    {
                        "name": "query_json",
                        "in": "path",
                        "description": "Query in JSON format",
                        "required": true,
                        "type": "string"
                      }
                ],
                "responses": {
                     "200": {
                        "description": "Successfully got search results ID",
                        "schema": {
                              "type": "object",
                              "properties": {
                              }
                        }
                     }
                }
            }
        }
        ,"/glycan/list/?query={query_json}": {
              "get": {
                "tags": [
                    "Glyan List"
                ],
                "description": "Get search results list",
                "parameters": [
                    {
                        "name": "query_json",
                        "in": "path",
                        "description": "Query in JSON format",
                        "required": true,
                        "type": "string"
                      }
                ],
                "responses": {
                     "200": {
                        "description": "Successfully got results list",
                        "schema": {
                              "type": "object",
                              "properties": {
                              }
                        }
                     }
                }
            }
        }
        ,"/glycan/detail/{glycan_id}": {
              "get": {
                "tags": [
                    "Glyan Details"
                ],
                "description": "Get glycan details",
                "parameters": [
                    {
                        "name": "glycan_id",
                        "in": "path",
                        "description": "Glycan ID",
                        "required": true,
                        "type": "string"
                      }
                ],
                "responses": {
                     "200": {
                        "description": "Successfully got glycan details",
                        "schema": {
                              "type": "object",
                              "properties": {
                              }
                        }
                     }
                }
            }
        }
        ,"/glycan/image/{glycan_id}": {
              "get": {
                "tags": [
                    "Glyan Image"
                ],
                "description": "Get glycan image",
                "parameters": [
                    {
                        "name": "glycan_id",
                        "in": "path",
                        "description": "Glycan ID",
                        "required": true,
                        "type": "string"
                      }
                ],
                "responses": {
                     "200": {
                        "description": "Successfully got glycan image",
                        "schema": {
                              "type": "object",
                              "properties": {
                              }
                        }
                     }
                }
            }
        }
    }
}
