{
    "$schema": "http://json-schema.org/schema#",
    "type": "object",
    "properties": {
        "glycan_mass": {
            "type": "object",
            "properties": {
                "native": {
                    "type": "object",
                    "properties": {
                        "name": {
                            "type": "string"
                        },
                        "min": {
                            "type": "number"
                        },
                        "max": {
                            "type": "number"
                        }
                    },
                    "required": [
                        "max",
                        "min",
                        "name"
                    ]
                },
                "permethylated": {
                    "type": "object",
                    "properties": {
                        "name": {
                            "type": "string"
                        },
                        "min": {
                            "type": "number"
                        },
                        "max": {
                            "type": "number"
                        }
                    },
                    "required": [
                        "max",
                        "min",
                        "name"
                    ]
                }
            },
            "required": [
                "native",
                "permethylated"
            ]
        },
        "number_monosaccharides": {
            "type": "object",
            "properties": {
                "min": {
                    "type": "integer"
                },
                "max": {
                    "type": "integer"
                }
            },
            "required": [
                "max",
                "min"
            ]
        },
        "organism": {
            "type": "array",
            "items": {
                "type": "object",
                "properties": {
                    "name": {
                        "type": "string"
                    },
                    "id": {
                        "type": "integer"
                    }
                },
                "required": [
                    "id",
                    "name"
                ]
            }
        },
        "glycan_type": {
            "type": "array",
            "items": {
                "type": "object",
                "properties": {
                    "name": {
                        "type": "string"
                    },
                    "subtype": {
                        "type": "array",
                        "items": {
                            "type": "string"
                        }
                    }
                },
                "required": [
                    "name",
                    "subtype"
                ]
            }
        },
        "id_namespace": {
            "type": "array",
            "items": {
                "type": "string"
            }
        },
        "simple_search_category": {
            "type": "array",
            "items": {
                "type": "object",
                "properties": {
                    "id": {
                        "type": "string"
                    },
                    "display": {
                        "type": "string"
                    }
                },
                "required": [
                    "display",
                    "id"
                ]
            }
        },
        "composition": {
            "type": "array",
            "items": {
                "type": "object",
                "properties": {
                    "residue": {
                        "type": "string"
                    },
                    "min": {
                        "type": "integer"
                    },
                    "max": {
                        "type": "integer"
                    },
                    "url": {
                        "type": "string"
                    }
                },
                "required": [
                    "max",
                    "min",
                    "residue",
                    "url"
                ]
            }
        },
        "biomarker_types": {
            "type": "array",
            "items": {
                "type": "string"
            }
        }
    },
    "required": [
        "composition",
        "glycan_mass",
        "glycan_type",
        "id_namespace",
        "number_monosaccharides",
        "organism",
        "simple_search_category",
        "biomarker_types"
    ]
}