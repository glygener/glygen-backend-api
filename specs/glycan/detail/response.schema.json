                "glytoucan_url": {
                    "type": "string",
                    "description": "glytoucan URL"
                }
            },
            "required": [
                "glytoucan_ac",
                "glytoucan_url"
            ],
            "description": "glytoucan object"
        },
        "mass": {
            "type": "number",
            "description": "monoisotopic mass of the glycan in daltons"
        },
        "number_monosaccharides": {
            "type": "integer",
            "description": "total number of monosaccharide residues"
        },
        "number_monosaccharides_suffix": {
            "type": "string",
            "description": "'+' used to indicate if glycan has repeating structure"
        },
        "iupac": {
            "type": "string",
            "description": "glycan sequence in iupac format"
        },
        "wurcs": {
            "type": "string",
            "description": "glycan sequence in wurcs format"
        },
        "glycoct": {
            "type": "string",
            "description": "glycan sequence in glycoct format"
        },
        "species": {
            "type": "array",
            "items": {
                "type": "object",
                "properties": {
                    "taxid": {
                        "type": "integer",
                        "description": "taxonomy id"
                    },
                    "name": {
                        "type": "string",
                        "description": "species name"
                    },
                    "annotation_category": {
                        "type": "string",
                        "description": "type of annotation"
