                    "protein_name": {
                        "type": "string",
                        "description": "protein name"
                    },
                    "gene": {
                        "type": "string",
                        "description": "gene name"
                    },
                    "gene_link": {
                        "type": "string",
                        "description": "gene url"
                    },
                    "tax_id": {
                        "type": "integer",
                        "description": "taxonomy id"
                    },
                    "tax_name": {
                        "type": "string",
                        "description": "taxonomy name"
                    },
                    "tax_common_name": {
                        "type": "string",
                        "description": "taxonomy common name"
                    },
                    "evidence": {
                        "type": "array",
                        "items": {
                            "type": "object",
                            "properties": {
                                "id": {
                                    "type": "string",
                                    "description": "evidence database id"
                                },
                                "database": {
                                    "type": "string",
                                    "description": "evidence database name"
                                },
                                "url": {
                                    "type": "string",
                                    "description": "evidence database url"
                                }
                            },
                            "required": [
                                "database",
                                "id"
                            ]
                        },
                        "description": "list of evidence sources"
