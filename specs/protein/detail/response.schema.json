{
   "$id":"http://example.com/example.json",
   "type":"object",
   "properties":{
      "uniprot_canonical_id":{
         "$id":"/properties/uniprot_canonical_id",
         "type":"string",
         "description":"ID of the protein using Uniprot entry name.",
         "examples":[
            "SHBG_HUMAN"
         ]
      },
      "uniprot_canonical_ac":{
         "$id":"/properties/uniprot_canonical_ac",
         "type":"string",
         "description":"Accession ID of the protein using Uniprot entry ID",
         "examples":[
            "P04278"
         ]
      },
      "mass":{
         "$id":"/properties/mass",
         "type":"number",
         "description":"Mass of the protein.",
         "examples":[
            50638
         ]
      },
      "recommendedName":{
         "$id":"/properties/recommendedName",
         "type":"object",
         "description":"Object with a tuple of short and long name for the protein. It is possible that one of the names is missing.",
         "properties":{
            "full":{
               "$id":"/properties/recommendedName/properties/full",
               "type":"string",
               "description":"Full name of the protein.",
               "examples":[
                  "Sex hormone-binding globulin"
               ]
            },
            "short":{
               "$id":"/properties/recommendedName/properties/short",
               "type":"string",
               "description":"Corresponding short name of the protein.",
               "examples":[
                  "SHBG"
               ]
            }
         }
      },
      "alternativeNames":{
         "$id":"/properties/alternativeNames",
         "type":"array",
         "description":"List of objects presenting the alternative names of the protein. Each object has a short and a full name. However its possible that one of the names is missing.",
         "items":{
            "$id":"/properties/alternativeNames/items",
            "type":"object",
            "description":"Each object has a short and a full name of the protein. However its possible that one of the names is missing.",
            "properties":{
               "full":{
                  "$id":"/properties/alternativeNames/items/properties/full",
                  "type":"string",
                  "description":"Alternative full name given to the protein.",
                  "examples":[
                     "Sex hormone-binding globulin"
                  ]
               },
               "short":{
                  "$id":"/properties/alternativeNames/items/properties/short",
                  "type":"string",
                  "description":"Corresponding alternative short name given to the protein.",
                  "examples":[
                     "SHBG"
                  ]
               }
            }
         }
      },
      "species":{
         "$id":"/properties/species",
         "type":"array",
         "description":"List of species annotation for the protein.",
         "items":{
            "$id":"/properties/species/items",
            "type":"object",
            "description":"Object with the species annotation. It contains the name, the NCBI Taxonomy ID and a list of evidences for the annotation.",
            "properties":{
               "name":{
                  "$id":"/properties/species/items/properties/name",
                  "type":"string",
                  "description":"Scientific name of the species.",
                  "default":"",
                  "examples":[
                     "homo sapiens"
                  ]
               },
               "taxid":{
                  "$id":"/properties/species/items/properties/taxid",
                  "type":"integer",
                  "description":"NCBI Taxonomy ID for the species entry.",
                  "default":0,
                  "examples":[
                     9606
                  ]
               },
               "evidence":{
                  "$id":"/properties/species/items/properties/evidence",
                  "type":"array",
                  "description":"List of evidence objects.",
                  "items":{
                     "$id":"/properties/species/items/properties/evidence/items",
                     "type":"object",
                     "description":"Evidence object that stores the database name, entry ID and URL. The objects refers to an entry on another database that provided this taxonomical annotation.",
                     "properties":{
                        "database":{
                           "$id":"/properties/species/items/properties/evidence/items/properties/database",
                           "type":"string",
                           "description":"Name of the database that provided this species annotation.",
                           "default":"",
                           "examples":[
                              "UniProt"
                           ]
                        },
                        "id":{
                           "$id":"/properties/species/items/properties/evidence/items/properties/id",
                           "type":"string",
                           "description":"Databases entry that provides this species annotation.",
                           "default":"",
                           "examples":[
                              "P04278"
                           ]
                        },
                        "url":{
                           "$id":"/properties/species/items/properties/evidence/items/properties/url",
                           "type":"string",
                           "description":"Link to the database entry that provided this species annotation.",
                           "default":"",
                           "examples":[
                              "http://glycomics.ccrc.uga.edu/ontologies"
                           ]
                        }
                     },
                     "required":[
                        "database"
                     ]
                  }
               }
            },
            "required":[
               "name"
            ]
         }
      },
      "gene":{
         "$id":"/properties/gene",
         "type":"array",
         "description":"List of objects describing the genes encoding the protein.",
         "items":{
            "$id":"/properties/gene/items",
            "type":"object",
            "description":"Gene object consists of the name of the gene and a reference to the gene entry.",
            "properties":{
               "name":{
                  "$id":"/properties/gene/items/properties/name",
                  "type":"string",
                  "title":"The Name Schema ",
                  "description":"Name of the gene encoding this protein.",
                  "default":"",
                  "examples":[
                     "MGAT1"
                  ]
               },
               "url":{
                  "$id":"/properties/gene/items/properties/url",
                  "type":"string",
                  "description":"Link to the entry of the Gene in a database (e.g. HGNC, GeneBank).",
                  "default":"",
                  "examples":[
                     "https://www.genenames.org/cgi-bin/gene_symbol_report?hgnc_id=HGNC:7044"
                  ]
               }
            }
         }
      },
      "crossref":{
         "$id":"/properties/crossref",
         "type":"array",
         "description":"List of references (database entries) that also represent this protein.",
         "items":{
            "$id":"/properties/crossref/items",
            "type":"object",
            "description":"A reference object consists of the name of the databases, the entry ID for this protein in this databases and a URL.",
            "properties":{
               "database":{
                  "$id":"/properties/crossref/items/properties/database",
                  "type":"string",
                  "description":"Name of the database with this protein entry.",
                  "default":"",
                  "examples":[
                     "PubChem Substance"
                  ]
               },
               "id":{
                  "$id":"/properties/crossref/items/properties/id",
                  "type":"string",
                  "description":"ID of this protein in the database.",
                  "default":"",
                  "examples":[
                     "252295406"
                  ]
               },
               "url":{
                  "$id":"/properties/crossref/items/properties/url",
                  "type":"string",
                  "description":"Link to the databases entry for this protein.",
                  "default":"",
                  "examples":[
                     "https://pubchem.ncbi.nlm.nih.gov/substance/252295406#section=Top"
                  ]
               }
            },
            "required":[
               "database"
            ]
         }
      },
      "sequence":{
         "$id":"/properties/sequence",
         "type":"object",
         "description":"Sequence object for the protein. It contains the amino acid sequence and the sequence length (number of amino acids.",
         "properties":{
            "sequence":{
               "$id":"/properties/sequence/properties/sequence",
               "type":"string",
               "description":"Amino acid sequence of the protein.",
               "default":"",
               "examples":[
                  "MESRGPLATSRLLLLLLLLLLRHTRQGWALRPVLPTQSAHDPPAVHLSNGPGQEPIAVMTFDLTKITKTSSSFEVRTWDPEGVIFYGDTNPKDDWFMLGLRDGRPEIQ"
               ]
            },
            "length":{
               "$id":"/properties/sequence/properties/length",
               "type":"integer",
               "description":"Number of amino acides in the sequence.",
               "examples":[
                  402
               ]
            }
         },
         "required":[
            "sequence"
         ]
      },
      "keywords":{
         "$id":"/properties/keywords",
         "type":"array",
         "description":"List of keywords that annotate the protein. The keyword concept is identical to the one in UniProt.",
         "items":{
            "$id":"/properties/keywords/items",
            "type":"string",
            "description":"String representing the keyword.",
            "examples":[
               "3D-structure"
            ]
         }
      },
      "isoforms":{
         "$id":"/properties/isoforms",
         "type":"array",
         "description":"List of isoforms of the protein.",
         "items":{
            "$id":"/properties/isoforms/items",
            "type":"object",
            "description":"Isoform object for the protein. Each object consists of an isoform ID, a sequence object and a URL to this isoform.",
            "properties":{
               "isoform_ac":{
                  "$id":"/properties/isoforms/items/properties/isoform_ac",
                  "type":"string",
                  "description":"ID of the isoform.",
                  "examples":[
                     "P04278-4"
                  ]
               },
               "locus":{
                  "$id":"/properties/isoforms/items/properties/locus",
                  "type":"object",
                  "description":"Location of the gene in the DNA.",
                  "properties":{
                     "chromosome":{
                        "$id":"/properties/isoforms/items/properties/locus/properties/chromosome",
                        "type":"string",
                        "description":"Name of the chromosome the gene is located on.",
                        "examples":[
                           "1",
                           "X"
                        ]
                     },
                     "start_pos":{
                        "$id":"/properties/isoforms/items/properties/locus/properties/start_pos",
                        "type":"integer",
                        "description":"Start position (number of base) where the encoded gene begins.",
                        "examples":[
                           1233131
                        ]
                     },
                     "end_pos":{
                        "$id":"/properties/isoforms/items/properties/locus/properties/end_pos",
                        "type":"integer",
                        "description":"End position (number of base) the encoded gene ends with.",
                        "examples":[
                           332423
                        ]
                     },
                     "evidence":{
                        "$id":"/properties/isoforms/items/properties/locus/properties/evidence",
                        "type":"array",
                        "description":"List of evidence objects.",
                        "items":{
                           "$id":"/properties/isoforms/items/properties/locus/properties/evidence/items",
                           "type":"object",
                           "description":"Evidence object that stores the database name, entry ID and URL. The objects refers to an entry on another database that provided this glycosylation annotation.",
                           "properties":{
                              "database":{
                                 "$id":"/properties/isoforms/items/properties/locus/properties/evidence/items/properties/database",
                                 "type":"string",
                                 "description":"Name of the database that provided this glycosylation annotation.",
                                 "default":"",
                                 "examples":[
                                    "UniProt"
                                 ]
                              },
                              "id":{
                                 "$id":"/properties/isoforms/items/properties/locus/properties/evidence/items/properties/id",
                                 "type":"string",
                                 "description":"Databases entry that provides this glycosylation annotation.",
                                 "default":"",
                                 "examples":[
                                    "P04278"
                                 ]
                              },
                              "url":{
                                 "$id":"/properties/isoforms/items/properties/locus/properties/evidence/items/properties/url",
                                 "type":"string",
                                 "description":"Link to the database entry that provided this glycosylation annotation.",
                                 "default":"",
                                 "examples":[
                                    "http://glycomics.ccrc.uga.edu/ontologies"
                                 ]
                              }
                           },
                           "required":[
                              "database"
                           ]
                        }
                     }
                  }
               },
               "sequence":{
                  "$id":"/properties/isoforms/items/properties/sequence",
                  "type":"object",
                  "description":"Sequence object that contains the amino acid sequence and the sequence length.",
                  "properties":{
                     "sequence":{
                        "$id":"/properties/isoforms/items/properties/sequence/properties/sequence",
                        "type":"string",
                        "description":"Amino acid sequence of the isoform.",
                        "examples":[
                           "MESRGPLATSRLLLLLLLLLLRHTRQGWALRPVLPTQSAHDPPAVHLSNGPGQEPIAVMTFDLTKITKTSSSFEVRTWD"
                        ]
                     },
                     "length":{
                        "$id":"/properties/isoforms/items/properties/sequence/properties/length",
                        "type":"integer",
                        "description":"Number of amino acids.",
                        "examples":[
                           402
                        ]
                     }
                  },
                  "required":[
                     "sequence",
                     "length"
                  ]
               },
               "url":{
                  "$id":"/properties/isoforms/items/properties/url",
                  "type":"string",
                  "description":"Link the webpage with this isoform.",
                  "examples":[
                     "https://pubchem.ncbi.nlm.nih.gov/substance/252295406#section=Top"
                  ]
               }
            },
            "required":[
               "id"
            ]
         }
      },
      "publication":{
         "$id":"/properties/publication",
         "type":"array",
         "description":"List of publications describing his protein.",
         "items":{
            "$id":"/properties/publication/items",
            "type":"object",
            "description":"A publication object consisting of a Pubmed ID, a title, the journal name and a URL to the paper.",
            "properties":{
               "pmid":{
                  "$id":"/properties/publication/items/properties/pmid",
                  "type":"string",
                  "description":"Pubmed ID of the paper.",
                  "examples":[
                     "2139487"
                  ]
               },
               "title":{
                  "$id":"/properties/publication/items/properties/title",
                  "type":"string",
                  "description":"Title of the paper.",
                  "examples":[
                     "Something about proteins"
                  ]
               },
               "journal":{
                  "$id":"/properties/publication/items/properties/journal",
                  "type":"string",
                  "description":"Name of the Journal the paper was published in.",
                  "examples":[
                     "Some Journal"
                  ]
               },
               "url":{
                  "$id":"/properties/publication/items/properties/url",
                  "type":"string",
                  "description":"Link to the paper.",
                  "examples":[
                     "https://pubchem.ncbi.nlm.nih.gov/substance/252295406#section=Top"
                  ]
               }
            }
         }
      },
      "glycosylation":{
         "$id":"/properties/glycosylation",
         "type":"array",
         "description":"List of glycosylations.",
         "items":{
            "$id":"/properties/glycosylation/items",
            "type":"object",
            "description":"Glycosylation object that contains the glycosylated amino acid, the positon of this amino acid, the reported glycan and a list of evidence for this annotation.",
            "properties":{
               "residue":{
                  "$id":"/properties/glycosylation/items/properties/residue",
                  "type":"string",
                  "description":"Amino acid residue that is glycosylated (one letter code).",
                  "examples":[
                     "T"
                  ]
               },
               "position":{
                  "$id":"/properties/glycosylation/items/properties/position",
                  "type":"integer",
                  "description":"Position of the amino acid residue in the protein sequence.",
                  "examples":[
                     36
                  ]
               },
               "glytoucan_ac":{
                  "$id":"/properties/glycosylation/items/properties/glytoucan_ac",
                  "type":"string",
                  "description":"ID of the glycan represented as GlyTouCan ID.",
                  "examples":[
                     "G123456"
                  ]
               },
               "type":{
                  "$id":"/properties/glycosylation/items/properties/type",
                  "type":"string",
                  "description":"Type of glycosylation (e.g. N-Glycan).",
                  "examples":[
                     "N-Glycan"
                  ]
               },
               "evidence":{
                  "$id":"/properties/glycosylation/items/properties/evidence",
                  "type":"array",
                  "description":"List of evidence objects.",
                  "items":{
                     "$id":"/properties/glycosylation/items/properties/evidence/items",
                     "type":"object",
                     "description":"Evidence object that stores the database name, entry ID and URL. The objects refers to an entry on another database that provided this glycosylation annotation.",
                     "properties":{
                        "database":{
                           "$id":"/properties/glycosylation/items/properties/evidence/items/properties/database",
                           "type":"string",
                           "description":"Name of the database that provided this glycosylation annotation.",
                           "default":"",
                           "examples":[
                              "UniProt"
                           ]
                        },
                        "id":{
                           "$id":"/properties/glycosylation/items/properties/evidence/items/properties/id",
                           "type":"string",
                           "description":"Databases entry that provides this glycosylation annotation.",
                           "default":"",
                           "examples":[
                              "P04278"
                           ]
                        },
                        "url":{
                           "$id":"/properties/glycosylation/items/properties/evidence/items/properties/url",
                           "type":"string",
                           "description":"Link to the database entry that provided this glycosylation annotation.",
                           "default":"",
                           "examples":[
                              "http://glycomics.ccrc.uga.edu/ontologies"
                           ]
                        }
                     },
                     "required":[
                        "database"
                     ]
                  }
               }
            },
            "required":[
               "type",
               "evidence"
            ]
         }
      },
      "function":{
         "$id":"/properties/function",
         "type":"array",
         "description":"List of functions that this protein has.",
         "items":{
            "$id":"/properties/function/items",
            "type":"object",
            "properties":{
               "annotation":{
                  "$id":"/properties/function/items/properties/annotation",
                  "type":"string",
                  "description":"Name of the function",
                  "examples":[
                     "Sex hormon binding protein"
                  ]
               },
               "evidence":{
                  "$id":"/properties/function/items/properties/evidence",
                  "type":"array",
                  "description":"List of evidence objects.",
                  "items":{
                     "$id":"/properties/function/items/properties/evidence/items",
                     "type":"object",
                     "description":"Evidence object that stores the database name, entry ID and URL. The objects refers to an entry on another database that provided this taxonomical annotation.",
                     "properties":{
                        "database":{
                           "$id":"/properties/function/items/properties/evidence/items/properties/database",
                           "type":"string",
                           "description":"Name of the database that provided this annotation.",
                           "default":"",
                           "examples":[
                              "UniProt"
                           ]
                        },
                        "id":{
                           "$id":"/properties/function/items/properties/evidence/items/properties/id",
                           "type":"string",
                           "description":"Databases entry that provides this annotation.",
                           "default":"",
                           "examples":[
                              "P04278"
                           ]
                        },
                        "url":{
                           "$id":"/properties/function/items/properties/evidence/items/properties/url",
                           "type":"string",
                           "description":"Link to the database entry that provided this annotation.",
                           "default":"",
                           "examples":[
                              "http://glycomics.ccrc.uga.edu/ontologies"
                           ]
                        }
                     },
                     "required":[
                        "database"
                     ]
                  }
               },
               "url":{
                  "$id":"/properties/function/items/properties/url",
                  "type":"string",
                  "description":"URL to a webpage describing the function.",
                  "default":"",
                  "examples":[
                     "http://glycomics.ccrc.uga.edu/ontologies"
                  ]
               }
            },
            "required":[
               "annotation",
               "evidence"
            ]
         }
      },
      "disease":{
         "$id":"/properties/disease",
         "type":"array",
         "description":"List of diseases the protein is involved in.",
         "items":{
            "$id":"/properties/disease/items",
            "type":"object",
            "properties":{
               "name":{
                  "$id":"/properties/disease/items/properties/name",
                  "type":"string",
                  "description":"Human readable name of the disease.",
                  "default":"",
                  "examples":[
                     "Osteoarthritis of first carpometacarpal joint"
                  ]
               },
               "icd10":{
                  "$id":"/properties/disease/items/properties/icd10",
                  "type":"string",
                  "description":"ICD10 number of the disease.",
                  "default":"",
                  "examples":[
                     "M18"
                  ]
               },
               "doid":{
                  "$id":"/properties/disease/items/properties/doid",
                  "type":"string",
                  "description":"Disease ontology ID of the disease.",
                  "default":"",
                  "examples":[
                     "DOID:162"
                  ]
               },
               "evidence":{
                  "$id":"/properties/disease/items/properties/evidence",
                  "type":"array",
                  "description":"List of evidence objects.",
                  "items":{
                     "$id":"/properties/disease/items/properties/evidence/items",
                     "type":"object",
                     "description":"Evidence object that stores the database name, entry ID and URL. The objects refers to an entry on another database that provided this taxonomical annotation.",
                     "properties":{
                        "database":{
                           "$id":"/properties/disease/items/properties/evidence/items/properties/database",
                           "type":"string",
                           "description":"Name of the database that provided this annotation.",
                           "default":"",
                           "examples":[
                              "UniProt"
                           ]
                        },
                        "id":{
                           "$id":"/properties/disease/items/properties/evidence/items/properties/id",
                           "type":"string",
                           "description":"Databases entry that provides this annotation.",
                           "default":"",
                           "examples":[
                              "P04278"
                           ]
                        },
                        "url":{
                           "$id":"/properties/disease/items/properties/evidence/items/properties/url",
                           "type":"string",
                           "description":"Link to the database entry that provided this annotation.",
                           "default":"",
                           "examples":[
                              "http://glycomics.ccrc.uga.edu/ontologies"
                           ]
                        }
                     },
                     "required":[
                        "database"
                     ]
                  }
               },
               "url":{
                  "$id":"/properties/disease/items/properties/url",
                  "type":"string",
                  "description":"URL of the entry in the disease ontology.",
                  "default":"",
                  "examples":[
                     "https://www.icd10data.com/ICD10CM/Codes/M00-M99/M15-M19/M18-/M18"
                  ]
               }
            },
            "required":[
               "name",
               "doid",
               "url"
            ]
         }
      },
      "mutation":{
         "$id":"/properties/mutation",
         "type":"array",
         "description":"List of mutations in the gene.",
         "items":{
            "$id":"/properties/mutation/items",
            "type":"object",
            "description":"Object describing a mutation.",
            "properties":{
               "annotation":{
                  "$id":"/properties/mutation/items/properties/annotation",
                  "type":"string",
                  "description":"Description of the mutation.",
                  "default":"",
                  "examples":[
                     "High frequency non-synonymous mutation in BRCA (DOID:001) cancer"
                  ]
               },
               "disease":{
                  "$id":"/properties/mutation/items/properties/disease",
                  "type":"object",
                  "properties":{
                     "name":{
                        "$id":"/properties/mutation/items/properties/disease/properties/name",
                        "type":"string",
                        "description":"Name of the disease.",
                        "default":"",
                        "examples":[
                           "Osteoarthritis of first carpometacarpal joint"
                        ]
                     },
                     "icd10":{
                        "$id":"/properties/mutation/items/properties/disease/properties/icd10",
                        "type":"string",
                        "title":"The Icd10 Schema ",
                        "description":"ICD10 ID of the disease.",
                        "default":"",
                        "examples":[
                           "M18"
                        ]
                     },
                     "doid":{
                        "$id":"/properties/mutation/items/properties/disease/properties/doid",
                        "type":"string",
                        "description":"Disease ontology ID of the disease.",
                        "default":"",
                        "examples":[
                           "DOID:162"
                        ]
                     },
                     "url":{
                        "$id":"/properties/mutation/items/properties/disease/properties/url",
                        "type":"string",
                        "description":"URL to the webpage of the disease ontology describing this disease.",
                        "default":"",
                        "examples":[
                           "https://www.icd10data.com/ICD10CM/Codes/M00-M99/M15-M19/M18-/M18"
                        ]
                     }
                  },
                  "required":[
                     "name",
                     "doid",
                     "url"
                  ]
               },
               "type":{
                  "$id":"/properties/mutation/items/properties/type",
                  "type":"string",
                  "description":"Type of mutation (point mutation, insertion, deletion).",
                  "default":"",
                  "examples":[
                     "point mutation"
                  ]
               },
               "start_pos":{
                  "$id":"/properties/mutation/items/properties/start_pos",
                  "type":"integer",
                  "description":"Start position of the mutation (base).",
                  "default":0,
                  "examples":[
                     36
                  ]
               },
               "end_pos":{
                  "$id":"/properties/mutation/items/properties/end_pos",
                  "type":"integer",
                  "description":"End position of the mutation (base).",
                  "default":0,
                  "examples":[
                     36
                  ]
               },
               "sequence_org":{
                  "$id":"/properties/mutation/items/properties/sequence_org",
                  "type":"string",
                  "description":"Orignal sequence (will be empty for insertion).",
                  "default":"",
                  "examples":[
                     "A"
                  ]
               },
               "sequence_mut":{
                  "$id":"/properties/mutation/items/properties/sequence_mut",
                  "type":"string",
                  "description":"Mutated sequence (will be empty for deletion).",
                  "default":"",
                  "examples":[
                     "G"
                  ]
               },
               "evidence":{
                  "$id":"/properties/mutation/items/properties/evidence",
                  "type":"array",
                  "description":"List of evidence objects.",
                  "items":{
                     "$id":"/properties/mutation/items/properties/evidence/items",
                     "type":"object",
                     "description":"Evidence object that stores the database name, entry ID and URL. The objects refers to an entry on another database that provided this taxonomical annotation.",
                     "properties":{
                        "database":{
                           "$id":"/properties/mutation/items/properties/evidence/items/properties/database",
                           "type":"string",
                           "description":"Name of the database that provided this annotation.",
                           "default":"",
                           "examples":[
                              "UniProt"
                           ]
                        },
                        "id":{
                           "$id":"/properties/mutation/items/properties/evidence/items/properties/id",
                           "type":"string",
                           "description":"Databases entry that provides this annotation.",
                           "default":"",
                           "examples":[
                              "P04278"
                           ]
                        },
                        "url":{
                           "$id":"/properties/mutation/items/properties/evidence/items/properties/url",
                           "type":"string",
                           "description":"Link to the database entry that provided this annotation.",
                           "default":"",
