{
   "$id":"http://example.com/example.json",
   "type":"object",
   "properties":{
      "glygan_mass":{
         "$id":"/properties/protein_mass",
         "type":"object",
         "description":"This object provides the minimum and maximum native mass of protein in the database.",
         "properties":{
            "min":{
               "$id":"/properties/protein_mass/properties/min",
               "type":"number",
               "description":"Smallest mass value of a protein in the repository.",
               "default":0,
               "examples":[
                435.00
               ]
            },
            "max":{
               "$id":"/properties/protein_mass/properties/max",
               "type":"integer",
               "description":"Largest mass value of a protein_mass in the repository.",
               "default":0,
               "examples":[
                3906488.00
               ]
            }
         },
         "required":[
            "min",
            "max"
         ]
      },
      "organism":{
         "$id":"/properties/organism",
         "type":"array",
         "description":"This field is set of organism (e.g. human) that a present in the databases. Each string should contain the scientific name and the the NCBI Taxonomy ID in parentheses.",
         "items":{
            "$id":"/properties/organism/items",
            "type":"string",
            "examples":[
               "Homo sapiens (Taxonomy ID: 9606)",
               "Mus musculus (Taxonomy ID: 10090)"
            ]
         }
      },
      "simple_search_category":{
         "$id":"/properties/simple_search_category",
         "type":"array",
         "description":"A list of categories for the simple search.",
         "items":{
            "$id":"/properties/simple_search_category/items",
            "type":"object",
            "description":"A category for the simple search which has an identifier (used for the search WS) and a display name.",
            "properties":{
               "display":{
                  "$id":"/properties/simple_search_category/items/properties/display",
                  "type":"string",
                  "description":"Name of the category used for display.",
                  "examples":[
                     "Glycan"
                  ]
               },
               "id":{
                  "$id":"/properties/simple_search_category/items/properties/id",
                  "type":"string",
                  "description":"Id of the category as used by the WS.",
                  "items":{
                     "$id":"/properties/simple_search_category/items/properties/subtype/items",
                     "type":"string",
                     "examples":[
                        "glycan"
                     ]
                  }
               }
            },
            "required":[
               "display",
               "id"
            ]
         }
      },
      "error_code":{
         "$id":"/properties/error_code",
         "type":"string",
         "description":"If the operation fails an error code is send back instead of the other information.",
         "default":"",
         "examples":[
            "ABV1234MN56BN"
         ]
      }
   }
}